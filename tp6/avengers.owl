<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://avengers.org/onto.owl"
         xmlns="http://avengers.org/onto.owl#">

<owl:Ontology rdf:about="http://avengers.org/onto.owl"/>

<owl:Class rdf:about="#Humain">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass rdf:resource="#Hero"/>
  <owl:equivalentClass rdf:resource="#Avenger"/>
</owl:Class>

<owl:Class rdf:about="#Hero">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass rdf:resource="#Avenger"/>
  <owl:equivalentClass rdf:resource="#Humain"/>
</owl:Class>

<owl:Class rdf:about="#Avenger">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:equivalentClass rdf:resource="#Hero"/>
  <owl:equivalentClass rdf:resource="#Humain"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#captainAmerica">
  <rdf:type rdf:resource="#Avenger"/>
  <rdf:type rdf:resource="#Hero"/>
  <rdf:type rdf:resource="#Humain"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#ironMan">
  <rdf:type rdf:resource="#Avenger"/>
  <rdf:type rdf:resource="#Hero"/>
  <rdf:type rdf:resource="#Humain"/>
</owl:NamedIndividual>


</rdf:RDF>
